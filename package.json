{
  "name": "Rogue-Dungeon-Explorer",
  "version": "1.0.0",
  "description": "dungeon game based off of the original 1980 platformer",
  "repository": "git@github.com:danielehrlich/Rogue-Dungeon-Explorer.git",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dev-server --config webpack.dev.config.js",
    "hot": "webpack-dev-server --progress --hot --inline --config webpack.hot.config.js",
    "build": "webpack -p --config webpack.prod.config.js",
    "build -e": "webpack --config webpack.prod.config.js --display-error-details",
    "build2": "webpack --config webpack.prod.config.js && uglify-js ./build/bundle.js -c -m -o ./build/bundle.min.js --source-map ./build/bundle.min.js.map",
    "test": "mocha --compilers js:babel-core/register --require ./test/helpers/browser.js --recursive ./test --timeout 15000 --reporter landing",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Daniel Ehrlich",
  "license": "ISC",
  "dependencies": {
    "jquery": "^2.2.4",
    "jsdom": "^8.1.0",
    "lodash": "^3.10.1",
    "react": "^15.3.2",
    "react-delay": "0.0.3",
    "react-dom": "^15.3.2",
    "react-notify-toast": "^0.1.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "redux": "^3.0.4",
  
    "autoprefixer": "^6.4.1",
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-stage-1": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.16.3",

    "css-loader": "^0.25.0",

    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",

    "node-sass": "^3.10.0",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^0.14.7",
    "redux-mock-store": "^1.2.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chai-jquery": "^2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-rallycoding": "^3.1.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.6",
    "webpack-dev-server": "^1.14.0"
    
  }
}
